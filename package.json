{
  "name": "gestionels",
  "version": "1.0.0",
  "private": true,
  "devDependencies": {
    "@google/clasp": "^2.5.0"
  },
  "scripts": {
    "clasp:push": "clasp push -f",
    "clasp:pull": "clasp pull",
    "clasp:open": "clasp open",
    "clasp:login": "clasp login",
    "clasp:logout": "clasp logout",
    "clasp:version": "node tools/version.js",
    "clasp:deploy": "node tools/deploy.js",
    "release": "pwsh tools/deploy-projet1.ps1",
    "snapshot": "pwsh tools/snapshot.ps1",
    "helper": "clasp-helper.cmd",
    "helper:ps": "pwsh ./clasp-helper.ps1",
    "assets:verify": "pwsh ./branding/ui/VERIFY-UI-ASSETS.ps1",
    "assets:build:b64": "pwsh ./branding/ui/Build-AssetEmbeds.ps1",
    "assets:embed:alu": "node ./scripts/embed-alu.mjs",
    "test": "node -e \"console.log('No automated tests configured. Use the Debug menu in Sheets or run: npm run test:clasp')\"",
    "test:clasp": "clasp run lancerTousLesTestsEtRetournerLogs --noninteractive"
  }
}

